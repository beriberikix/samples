&i2c2 {
	compatible = "nordic,nrf-twim";
	status = "okay";
	sda-pin = <30>;
	scl-pin = <31>;
	clock-frequency = <I2C_BITRATE_FAST>;

	bh1750@23 {
		compatible = "rohm,bh1750";
		label = "BH1750";
		reg = <0x23>;
			
	};
};

